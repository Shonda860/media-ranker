<h2>List of User</h2><br>
    <table class="table">
      <thead>
        <tr>
          <th>Username</th>
          <th>Votes</th>
          <th>Joined</th>
        </tr>
      </thead>
          
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.username,user_path(user)  %></td>
            <td><%= 0 %></td>
            <td><%= user.joined %></td>
          </tr> 
        <% end %> 
      </tbody>
    </table>

