<h2>List of User</h2><br>

  <div class="containter-lg">

    <table class="table table-hover">
      <thead>
        <tr>
          <th>Username</th>
          <th>Votes</th>
          <th>Joined</th>
        </tr>
      <thead>
          
      <tbody>
        <% @users.each do |user| %>
          <tr class="clickable-tr" data-path="<%=user_path(user)%>">
            <td><%= user.username %></td>
            <td><%= user.votes %></td>
            <td><%= user.joined %></td>
            
          </tr> 
        <% end %> 
      </tbody>
    </table>
    </div>
