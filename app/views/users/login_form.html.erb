<h2>Log In</h2>
  
  <%= form_with model: @user, class:"login", url: login_path do |f|%>
    <div class="form-group">
      <%= f.label :username%><br>
      <%= f.text_field :username%>
    </div>
    <div class="form-group">
      <%= f.submit "Log In", class: "btn btn-primary"%>
    </div>
  <% end %>
